<div class="elevated my-5 p-4">
  
  <%= form_tag url_for(controller: "questions", action: "index"), method: :get do  %>
    <!--Slider from https://mdbootstrap.com/docs/b4/jquery/forms/slider/-->
    <div class="d-flex justify-content-between align-items-center my-4">
      <label>Number of questions</label>
      <div class="w-50 d-flex justify-content-end">
        <input type="range" class="custom-range w-75" name="num_questions" id="customRange11" min="4" max="8">
        <span class="font-weight-bold text-light ml-2 valueSpan2"></span>
      </div>
    </div>
      
    <%= submit_tag "Start quiz", class:"w-100 btn-outline-light" %>
  <%end%>
  
  
</div>
